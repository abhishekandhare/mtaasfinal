    <script>
               var app=angular.module('MtaaS', []);
             app.controller('MtaaS', ['$scope', '$http', '$window', function ( $scope,$http,$window)
             {

                 //Add Project Code
                 
                 	$scope.addproject = function (){
				var in_appmanagerid = $scope.appmanagerid;
				var in_appmanagercompany = $scope.appmanagercompany;
				var in_appname = $scope.appname;
				var in_appurl = $scope.appurl;
				var in_testurl=$scope.testurl;
				var in_testdesc=$scope.testdesc;
				var in_startdate= $scope.startdate;
                var in_enddate= $scope.enddate;


	$http({
			  method: 'POST',
			  url: '/addproject',
			  data: {"appmanagerid":in_appmanagerid,"appmanagercompany": in_appmanagercompany,"appname":in_appname,"appurl":in_appurl,"testurl":in_testurl,"testdesc":in_testdesc,"startdate":in_startdate,"enddate":in_enddate}
			}).
			  then(function(response) {

				  $window.location.href = '/searchsupplier';
			  });
                        
                        $scope.appname="";
}
                 
                 
                 //Add Project Code Ends here
                 
                 
                 
                 
              //Get App Manager Details   
                      $http({
			      	  method: 'GET',
			      	  url: '/appmanagerusername'
			      	  
			        }).
			          then(function(response) {
			         //alert(response.data.result);
			         $scope.appmanagerusername = angular.fromJson(response.data.result);
                     $scope.appmanagerid = $scope.appmanagerusername[0].appmanagerid;
                     $scope.appmanagercompany = $scope.appmanagerusername[0].company;
			          });
            
            //Get Supplier Details
            $scope.searchsupplierdetails = function() {
                            $http({
                                method: 'GET',
                                url: '/searchsupplierdetails',
                                }).success(function(response){
                                    //alert(JSON.stringify(response));
                                    $scope.suppliers= JSON.parse(response.result);
                            }).error(function(error){
                                alert("ERROR VIEWING PENDING CUSTOMER REQUEST");
                            });
                        }; 
                     $scope.searchsupplierdetails(); 

              $http({
             method: 'GET',
             url: '/checklogin'
             }).
             then(function(response)
             {
                if(response.data.status===300)
                {
                $window.location.href='/';
                }
             });




              $scope.logoutsession= function () {
             //alert("in logout session");
             $http({
             method: 'GET',
             url: '/logoutsession'
             }).
            then(function(response)
             {
                if(response.data.status===300)
                {
                console.log("destroy session");
                }else
                {
                $window.location.href='/';
                }
             });
             } 



                $scope.addsupplier = function (){
			   $window.location.href = '/addsupplier';
			}
			  $scope.editsupplier = function (){
			   $window.location.href = '/editsupplier';
			}
			  $scope.searchsupplier = function (){
			   $window.location.href = '/searchsupplier';
              }
              
			 $scope.processsupplier = function (){
			   $window.location.href = '/processsupplier';
			}
             /*$scope.reports = function (){
			   $window.location.href = '/reportsmanager';
			}*/
             
             $scope.profile = function (){
			   $window.location.href = '/profilemanager';
			}
             
             $scope.invoice = function (){
			   $window.location.href = '/invoicemanager';
			}
             $scope.evaluate = function (){
			   $window.location.href = '/evaluatemanager';
			}

              }]);
  </script>
